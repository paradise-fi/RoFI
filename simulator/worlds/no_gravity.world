<sdf version="1.6">
  <world name='default'>
    <!-- Remove gravity -->
    <gravity>0 0 0</gravity>

    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Attacher plugin -->
    <plugin name="attacher" filename="libattacherPlugin.so"/>
  </world>
</sdf>
