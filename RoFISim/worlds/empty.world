<sdf version='1.6'>
  <world name='default'>
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Attacher plugin -->
    <plugin name='attacher' filename='libattacherPlugin.so'/>
  </world>
</sdf>
